<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.calculator_bymadhavi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CalcuLatorByMadhavi">
        <activity android:name=".Subtraction"
            android:label="Subtraction"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".Addition"
            android:label="Addition"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Division"
            android:label="Division"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Multiplication"
            android:label="Multiplication"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>